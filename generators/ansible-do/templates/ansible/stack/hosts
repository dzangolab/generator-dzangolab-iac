[swarm_leader]
leader ansible_host=LEADER_IP_ADDRESS

[swarm_workers]
worker1 ansible_host=WORKER1_IP_ADDRESS
worker2 ansible_host=WORKER2_IP_ADDRESS

[swarm:children]
swarm_leader
swarm_workers

[swarm:vars]
ansible_python_interpreter=/usr/bin/python3
ansible_user=<%= username %>

[apps:children]
swarm

[awscli:children]
swarm

[db]
leader

[portainer]
leader

